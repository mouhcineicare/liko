  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8048' next dev",
    "build": "NEXT_PUBLIC_APP_VERSION=$npm_package_version next build",
    "start": "next start",
    "cron": "node ./lib/cron/runner.js",
    "dev:with-cron": "concurrently \"yarn dev\" \"yarn cron\"",
    "start:with-cron": "concurrently \"yarn start\" \"yarn cron\"",
    "lint": "next lint",
    "init-admin": "node scripts/init-admin.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll",
    "migrate:recurring": "node scripts/migration.js --upgrade-recurring",
    "git:pull": "echo 'Pulling latest code...' && git pull",
    "clean": "echo 'Cleaning .next...' && rm -rf .next",
    "pm2:restart": "echo 'Restarting PM2 app...' && pm2 restart 0 --node-args='--max-old-space-size=2048'",
    "mongod:restart": "echo 'Restarting MongoDB...' && sudo systemctl restart mongod",
    "setup": "yarn git:pull && yarn clean && yarn install && yarn build && yarn pm2:restart && yarn mongod:restart",
    "prepare": "husky install",
    "protect": "node scripts/obfuscate.js",
    "unprotect": "node scripts/restore.js",
    "test:appointments:js": "node scripts/test-all-appointments-flow.js"
